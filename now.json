{
    "name": "now-test",
    "version": 2,
    "builds": [
        { "src": "server/index.js", "use": "@now/node-server" },
        { "src": "next-client/package.json", "use": "@now/static-build", "config": { "distDir": "dist" }}
    ],
    "routes": [
        { "src": "/graphql", "dest": "server/index.js" },
        { "src": "/(.*)", "dest": "next-client/$1"}
    ]
}