{
    "name": "aa2gd",
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@now/node-server"
      },
      {
        "src": "client/next.config.js",
        "use": "@now/next"
      }
    ],
    "routes": [
      {
        "src": "/graphql",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/$1"
      }
    ],
    "env": {
        "MONGO_URL": "@aa2gd-mongo-url",
        "JWT_SECRET": "@aa2gd-jwt-secret"
    }
  }